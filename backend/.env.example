# Database connection
# For local development with PostgreSQL:
# DATABASE_URL="postgresql://username:password@localhost:5432/ramboo?schema=public"
# For SQLite (local development only - change schema.prisma provider to "sqlite"):
# DATABASE_URL="file:./dev.db"

DATABASE_URL="postgresql://username:password@localhost:5432/ramboo?schema=public"

# JWT Secret - generate a strong random string for production
JWT_SECRET="ramboo-engineering-secret-key-change-in-production"

# Server configuration
PORT=3001
NODE_ENV=development

# CORS - Frontend URL (for production, set your actual domain)
CORS_ORIGIN="http://localhost:5173"
